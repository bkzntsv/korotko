# Telegram Bot Token
# Получить можно у @BotFather в Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# OpenAI API Key
# Получить можно на https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# ZenRows API Key
# Для обхода блокировок (например, LinkedIn)
ZENROWS_API_KEY=8eb2686eca5f17574b0146e21d26817f11c29bbf

# LinkedIn Cookie (li_at)
# Для доступа к полным профилям (нужен, если ZenRows не видит данные)
# Взять из браузера: Developer Tools -> Application -> Cookies -> li_at
LINKEDIN_COOKIE=

# User Agent (опционально, по умолчанию используется стандартный)
USER_AGENT=Mozilla/5.0 (compatible; Bot/1.0)
